<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
    
    <servlet>
        <servlet-name>YuRongServlet</servlet-name>
        <servlet-class>com.cfx.yurong.web.servlet.YuRongServlet</servlet-class>
    </servlet>
    
    <servlet-mapping>
        <servlet-name>YuRongServlet</servlet-name>
        <url-pattern>/yuRong</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>FbpServlet</servlet-name>
        <servlet-class>com.cfx.yurong.web.servlet.FbpServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>FbpServlet</servlet-name>
        <url-pattern>/fbp</url-pattern>
    </servlet-mapping>
</web-app>